{
  "name": "ts-express-boilerplate",
  "version": "0.0.1",
  "description": "Node.js express typescript boilerplate",
  "readme": "README.md",
  "scripts": {
    "start": "ts-node src/app.ts",
    "start:dev": "nodemon src/app.ts",
    "start:prod": "npm run build && NODE_ENV=production node dist/app.js",
    "test": "jest",
    "test:cov": "jest --coverage",
    "com:git": "git add . && git commit -m ",
