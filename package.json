{
  "name": "ts-express-boilerplate",
  "version": "0.0.1",
  "description": "Node.js express typescript boilerplate",
  "readme": "README.md",
  "scripts": {
    "start": "ts-node src/index.ts",
    "start:dev": "nodemon src/index.ts",
    "start:prod": "npm run build && NODE_ENV=production node dist/index.js",
    "test": "jest",
    "test:cov": "jest --coverage",
    "com:git": "git add . && git commit -m ",
    "push:git": "git push",
    "build": "npx tsc",
    "lint": "eslint . --ext .ts"
  },
  "author": {
    "name": "nedim akar",
    "email": "nedim.akar53411@gmail.com"
  },
  "license": "MIT",
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/node": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^5.29.0",
    "@typescript-eslint/parser": "^5.29.0",
    "eslint": "^8.18.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "nodemon": "^2.0.16",
    "prettier": "^2.7.1",
    "ts-node": "^10.8.1",
    "typescript": "^4.7.4"
  },
  "dependencies": {
    "express": "^4.18.1"
  }
}